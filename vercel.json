{
    "version": 2,
    "functions": {
      "api/**/*.ts": {
        "maxDuration": 60
      }
    },
    "builds": [
      {
        "src": "src/index.ts",
        "use": "@vercel/node"
      }
    ],
    "buildCommand": "npm run prisma:generate && npm run build",
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/index.ts"
      }
    ]
  }
  